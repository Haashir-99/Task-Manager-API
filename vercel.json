{
  "version": 2,
  "builds": [{ "src": "app.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/(.*)", "dest": "app.js" },
    { "src": "/api/docs/(.*)", "dest": "/node_modules/swagger-ui-dist/$1" },
    {
      "src": "/api/docs/swagger-ui.css",
      "dest": "/node_modules/swagger-ui-dist/swagger-ui.css"
    },
    {
      "src": "/api/docs/swagger-ui-bundle.js",
      "dest": "/node_modules/swagger-ui-dist/swagger-ui-bundle.js"
    },
    {
      "src": "/api/docs/swagger-ui-standalone-preset.js",
      "dest": "/node_modules/swagger-ui-dist/swagger-ui-standalone-preset.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "X-Content-Type-Options", "value": "nosniff" }]
    }
  ]
}
